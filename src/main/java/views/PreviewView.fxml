<BorderPane prefHeight="344.0" prefWidth="566.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="views.PreviewController">
   <center>
      <ScrollPane prefHeight="400.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <content>
            <VBox prefHeight="296.0" prefWidth="565.0">
               <children>
                  <Label fx:id="planNameLabel" alignment="CENTER" contentDisplay="CENTER" maxWidth="1.7976931348623157E308" style="-fx-border-style: solid none solid none; -fx-border-color: black;" text="Title" textAlignment="CENTER">
                     <font>
                        <Font size="36.0" />
                     </font>
                  </Label>
                  <VBox fx:id="sectionContainer" prefHeight="200.0" prefWidth="100.0" />
               </children>
               <padding>
                  <Insets left="40.0" right="40.0" />
               </padding>
            </VBox>
         </content>
      </ScrollPane>
   </center>
   <top>
      <HBox prefHeight="46.0" prefWidth="566.0" BorderPane.alignment="CENTER">
         <children>
            <Button id="goBackBtn" fx:id="goBackBtn" mnemonicParsing="false" onAction="#onClickGoback" text="Go back">
               <HBox.margin>
                  <Insets left="20.0" top="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </top>
